<library path="lib/libjoint_trajectory_admittance_controller">

  <class name="position_controllers/JointTrajectoryAdmittanceController"
         type="position_controllers::JointTrajectoryAdmittanceController"
         base_class_type="controller_interface::ControllerBase">
    <description>
      The JointTrajectoryController executes joint-space trajectories on a set of joints.
      This variant represents trajectory segments as quintic splines and sends commands to a position interface.
    </description>
  </class>

  <class name="velocity_controllers/JointTrajectoryAdmittanceController"
         type="velocity_controllers::JointTrajectoryAdmittanceController"
         base_class_type="controller_interface::ControllerBase">
    <description>
      The JointTrajectoryController executes joint-space trajectories on a set of joints.
      This variant represents trajectory segments as quintic splines and sends commands to a velocity interface.
    </description>
  </class>

  <class name="effort_controllers/JointTrajectoryAdmittanceController"
         type="effort_controllers::JointTrajectoryAdmittanceController"
         base_class_type="controller_interface::ControllerBase">
    <description>
      The JointTrajectoryController executes joint-space trajectories on a set of joints.
      This variant represents trajectory segments as quintic splines and sends commands to an effort interface.
    </description>
  </class>

</library>
